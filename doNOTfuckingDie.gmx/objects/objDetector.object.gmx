        <arguments>
          <argument>
            <kind>1</kind>
            <string>for(var i = 0; i &lt; detect_area; i++)
{
    for(var j = 0; j &lt; detect_area; j++)
    {
        for(var o = 0; o &lt; array_length_1d(detect_list); o++)
        {
            var xx = (mouse_x-(detect_area*point_radius*2)/2)+(i*point_radius*2);
            var yy = (mouse_y-(detect_area*point_radius*2)/2)+(j*point_radius*2);
            var obj = detect_list[o];
            var col = off_col;
            if(collision_point(xx, yy, obj, true, true))
                col = on_col;
            draw_set(point_alpha);
            draw_circle_colour(xx, yy, point_radius, col, merge_colour(col, c_black, 0.1), false);
            draw_set();
        }
        
    }
}

</string>
