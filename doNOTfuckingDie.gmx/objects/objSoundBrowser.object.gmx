          <argument>
            <kind>1</kind>
            <string>/// browser through sounds


str = "Navigate sounds with left/right.";
str += "#Press space to play selected sound.";
str += "#Press enter to stop all sounds.";
str += "#Sound ["+string(index_selected)+"] selected: " + audio_get_name(index_selected);


var next_audio = keyboard_check_pressed(vk_right);
var prev_audio = keyboard_check_pressed(vk_left);
var play_audio = keyboard_check_pressed(vk_space);
var stop_all = keyboard_check_pressed(vk_enter);

if(next_audio) index_selected++;
if(prev_audio) index_selected--;
index_selected = clamp(index_selected, 0, ds_list_size(sounds)-1);

if(play_audio)
    audio_play_sound(ds_list_find_value(sounds, index_selected),0,0);
    
if(stop_all)
    audio_stop_all();
    

</string>
