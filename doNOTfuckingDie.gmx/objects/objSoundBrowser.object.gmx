            <string>/// make sure "ZZZ_last_sound" is last on the list

x = view_xview + view_wview/2;
y = view_yview + view_hview/2;

sounds = ds_list_create();
str = "";

var len = ZZZ_last_sound;
for(var i = 0; i &lt; len; i++)
{    
    if(audio_exists(i))
        ds_list_add(sounds, i);
    else
        show_debug_message("Audio ["+string(i)+"] doesnt exist: " + audio_get_name(i) );
}

index_selected = 0;
